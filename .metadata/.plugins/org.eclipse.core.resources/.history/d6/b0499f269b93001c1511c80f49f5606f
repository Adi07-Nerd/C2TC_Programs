package com.tns.framework;

public abstract class NormalAcc extends ShopAcc{
//	private final float deliveryCharges; there is no need of this

//	public float getDeliveryCharges() {
//		return deliveryCharges;
//	}

	public NormalAcc(int accNo, String accNm, float charges) {
		super(accNo, accNm, charges);
//		this.deliveryCharges = deliveryCharges;
	}

	@Override
	public void bookProduct(float charges) {
		System.out.println("Dear "+this.accNm+" Your Charges are :"+charges+" with Delivery Charges is :"+getCharges());	
	}

	@Override
	public String toString() {
		return "NormalAcc [deliveryCharges=" + getCharges() + ", AccNo=" + getAccNo() + ", AccNm="+ getAccNm() + "]";
	}
	
}
